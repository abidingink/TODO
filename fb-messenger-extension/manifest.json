{
  "manifest_version": 3,
  "name": "@fred Bot - Standalone",
  "version": "5.1.0",
  "description": "Standalone version - no content scripts needed",
  
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  
  "action": {
    "default_popup": "popup.html",
    "default_title": "@fred Bot - Standalone"
  }
}